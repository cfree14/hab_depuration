@article{ WOS:A1991GQ75700005,
Author = {ROBINEAU, B and FORTIER, L and GAGNE, JA and CEMBELLA, AD},
Title = {COMPARISON OF THE RESPONSE OF 5 LARVAL FISH SPECIES TO THE TOXIC
   DINOFLAGELLATE ALEXANDRIUM-EXCAVATUM (BRAARUD) BALECH},
Journal = {JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY},
Year = {1991},
Volume = {152},
Number = {2},
Pages = {225-242},
Abstract = {The response of fish larvae to the toxic dinoflagellate Alexandrium
   excavatum is reviewed based on published and new data from comparable
   experiments in which larvae were exposed to various concentrations of
   toxic A. excavatum (treatment) and nontoxic A. tamarense (control).  In
   direct intoxication experiments (larvae fed toxic cells), severity of
   symptoms, response time and toxin-related mortality (7-96\%.day-1)
   depended on species sensitivity, dinoflagellate strain toxicity and
   toxic cell concentration. In vectorial intoxication experiments
   (postlarvae fed tainted zooplankton), the mortality attributable to the
   toxins was lower and less variable. Elimination of the toxins by the
   zooplankton could limit the propagation of dinoflagellate toxins towards
   higher levels of the planktonic food web. It is concluded that the
   potential impact of A. excavatum blooms on fish survival in nature will
   be stronger during the phytophagous larval and the early postlarval
   stages when dinoflagellate cells are ingested directly. Along the
   northwest Atlantic seaboard, the threat to coastal fisheries from A.
   excavatum blooms is predicted to intensify from south to north, along a
   gradient of increasing toxicity of dinoflagellate strains.},

@article{ WOS:A1960WS73100012,
Author = {SHIRODKAR, MV and WARWICK, A and BANG, FB},
Title = {THE INVITRO REACTION OF LIMULUS AMEBOCYTES TO BACTERIA},
Journal = {BIOLOGICAL BULLETIN},
Year = {1960},
Volume = {118},
Number = {2},
Pages = {324-337},
Abstract = {The defense mechanisms of Limulus against bacterial infection were
   studied in vitro in siliconized and unsiliconized blood culture systems
   developed for long-term amebocyte maintenance (30 days in an intact
   state and 36 days in an extended, partially de-granulated condition in
   the 2 systems respectively). Cell multiplication was not observed and
   replenishment of the medium or use of antibiotics were unnecessary. The
   pathogenicity of a Vibrio (or its thermostable toxin) for the
   siliconized (intact) system was established, for even 10-100 inoculated
   organisms could infect and destroy the cultures, persistent gel
   formation and amebocyte cytopathology being regularly demonstrable
   resultant phenomena. Using a nonpathogenic Alkaligenes- like bacterium,
   it was shown that only the siliconized cultures could rapidly (6 hr. at
   10-25{[}degree]C) and permanently eliminate large inocula (2.4 x 107
   organisms), gel formation being absent. These cultures were also active
   against several other marine bacteria. The unsiliconized and whole blood
   homogenate systems exhibited varying degrees of evanescent activity. The
   possible significance of these findings in relation to the suggestion
   that defense mechanisms against hemorrhage (gelation, clotting) and
   against infection (production of antibacterial substance or substances)
   may be intimately linked between the cellular and fluid elements of
   Limulus blood is discussed.},

@article{ WOS:000277482700025,
Author = {Alonso, E. and Vale, C. and Sasaki, M. and Fuwa, H. and Konno, Y. and
   Perez, S. and Vieytes, M. R. and Botana, L. M.},
Title = {Calcium Oscillations Induced by Gambierol in Cerebellar Granule Cells},
Journal = {JOURNAL OF CELLULAR BIOCHEMISTRY},

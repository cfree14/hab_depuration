@article{ WOS:000165536400003,
Author = {Bonafede, SJ and Brady, RF},
Title = {Compositional effects on the fouling resistance of fluorourethane
   coatings},
Journal = {JOCCA-SURFACE COATINGS INTERNATIONAL},
Year = {1998},
Volume = {81},
Number = {4},
Pages = {181+},
Month = {APR},
Abstract = {The property perhaps most frequently associated with fluorinated
   polymers is their ability to prevent or limit adhesive bonding to
   themselves. This property forms the basis for such commercial and
   industrial applications as nonstick cooking utensils, finishes for
   carpets, clothing and leather goods, prosthetic devices and surgical
   implants, magnetic recording material, and surface coatings for
   industrial, architectural, automotive and marine use. This work
   concentrates on coatings which resist strong bonds to marine organisms
   and discourage the attachment of marine growth to the hull of a ship.
   The aim is to develop a coating that weakens or eliminates the adhesive
   bond between fouling organisms and the coating. Then the fouling would
   be dislodged by relatively weak mechanical forces such as the weight of
   the fouling or the sheer as the ship moves through the water. The
   intention is to avoid toxins such as heavy metals which may accumulate

@article{ WOS:000304676800011,
Author = {Lage, Sandra and Raimundo, Joana and Brotas, Vanda and Costa, Pedro Reis},
Title = {Detection and sub-cellular distribution of the amnesic shellfish toxin,
   domoic acid, in the digestive gland of Octopus vulgaris during
   periods of toxin absence},
Journal = {MARINE BIOLOGY RESEARCH},
Year = {2012},
Volume = {8},
Number = {8},
Pages = {784-789},
Abstract = {Domoic acid (DA) is a neurotoxin produced by Pseudo-nitzschia diatoms
   that is accumulated in the highest concentrations in filter-feeding
   organisms, such as bivalve molluscs. Although DA is a water-soluble
   molecule that is rapidly cleared from most marine organisms, it appears
   that octopus may consistently contain DA in the digestive gland. To test
   this hypothesis, specimens of Octopus vulgaris were caught for DA
   analysis when Pseudo-nitzschia cells were not observed in seawater and
   DA was not detected in octopus food items, i.e. mussels. DA was
   consistently detected in each octopus specimen (n = 32) analysed and its
   concentrations varied from 1.0 to 26.6 mu g g(-1). DA sub-cellular
   distribution was evaluated in the digestive gland of six specimens in
   order to prove whether DA preferably accumulates in some cell
   compartment. DA was predominantly found (> 90\%) in the soluble fraction
   (cytosol) and to a lesser extent in the insoluble fractions (nucleic,
   mitochondrial, lysosomal, microsomal). This disposition should favour DA
   trophic transfer from prey to predators. The present study contributes
   with relevant data to the growing knowledge on the accumulation of
   marine toxins in cephalopods and on the trophic transfer of DA
   throughout the marine food web.},
Publisher = {TAYLOR \& FRANCIS AS},
Address = {KARL JOHANS GATE 5, NO-0154 OSLO, NORWAY},
Type = {Article},
Language = {English},
Affiliation = {Costa, PR (Corresponding Author), IPIMAR Natl Inst Biol Resources, Av Brasilia, P-1449006 Lisbon, Portugal.
   Lage, Sandra; Raimundo, Joana; Costa, Pedro Reis, IPIMAR Natl Inst Biol Resources, P-1449006 Lisbon, Portugal.
   Brotas, Vanda, Univ Lisbon, Ctr Oceanog, Fac Ciencias, P-1699 Lisbon, Portugal.},
DOI = {10.1080/17451000.2012.659668},
ISSN = {1745-1000},
EISSN = {1745-1019},

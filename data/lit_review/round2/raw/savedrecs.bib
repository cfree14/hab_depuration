@article{ WOS:000595376400001,
Author = {Landrigan, Philip J. and Stegeman, John J. and Fleming, Lora E. and
   Allemand, Denis and Anderson, Donald M. and Backer, Lorraine C. and
   Brucker-Davis, Francoise and Chevalier, Nicolas and Corra, Lilian and

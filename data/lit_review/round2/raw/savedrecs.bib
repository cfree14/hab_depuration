@article{ WOS:000328622500014,
Author = {Pizarro, Gemita and Morono, Angeles and Paz, Beatriz and Franco, Jose M.
   and Pazos, Yolanda and Reguera, Beatriz},
Title = {Evaluation of Passive Samplers as a Monitoring Tool for Early Warning of
   Dinophysis Toxins in Shellfish},

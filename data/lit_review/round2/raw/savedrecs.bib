@article{ WOS:000537151000018,
Author = {Jaramillo, Marcela and Joens, Jeffrey A. and O'Shea, Kevin E.},
Title = {Fundamental Studies of the Singlet Oxygen Reactions with the Potent
   Marine Toxin Domoic Acid},
Journal = {ENVIRONMENTAL SCIENCE \& TECHNOLOGY},
Year = {2020},
Volume = {54},
Number = {10},
Pages = {6073-6081},
Month = {MAY 19},
Abstract = {Domoic acid (DA), a potent marine toxin, is readily oxidized upon
   reaction with singlet oxygen (O-1(2)). Detailed product studies revealed
   that the major singlet oxygenation reaction pathways were the {[}2 + 2]
   cycloaddition (60.2\%) and ene reactions (39.8\%) occurring at the Z
   double bond. Diene isomerization and {[}4 + 2] cycloaddition, common for
   conjugated diene systems, were not observed during the singlet
   oxygenation of DA. The bimolecular rate constant for the DA reaction
   with(1)O(2) determined by competition kinetics was 5.1 x 10(5)
   M(-1)s(-1). Based on the rate constant and steady-state concentrations
   of O-1(2) in surface waters, the environmental half-life of DA due to
   singlet oxygen-induced transformations is between 5 and 63 days.
   The(1)O(2) reaction product mixture of DA did not exhibit significant
   biological activity based on ELISA studies, indicating that singlet
   oxygenation could be an important natural detoxification process. The
   characteristic oxidation products can provide valuable markers for the
   risk assessment of DA-contaminated natural waters.},

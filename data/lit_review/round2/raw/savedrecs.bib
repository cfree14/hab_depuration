@article{ WOS:A1993MX61200037,
Author = {HORNER, RA and KUSSKE, MB and MOYNIHAN, BP and SKINNER, RN and WEKELL,
   JC},
Title = {RETENTION OF DOMOIC ACID BY PACIFIC RAZOR CLAMS, SILIQUA-PATULA (DIXON,
   1789) - PRELIMINARY-STUDY},
Journal = {JOURNAL OF SHELLFISH RESEARCH},
Year = {1993},
Volume = {12},
Number = {2},
Pages = {451-456},
Month = {DEC},
Abstract = {Domoic acid concentrations up to 160 mug g-1 shellfish meat were
   reported in razor clams on the  Washington/Oregon coasts in the fall of
   1991. Toxin levels in the clams remained above the regulatory closure
   level of 20 mug g-1 for at least 6 months. In summer, 1992, razor clams,
   averaging about 10 mug g-1 of domoic acid toxin, were maintained under
   laboratory conditions to determine bow long it would take them to be
   free of the toxin. Periodically, edible (foot, siphon, and mantle) and
   non-edible (gill, digestive gland, and gonad) parts were tested for
   domoic acid. After 86 days, toxin levels remained near the original
   levels, but at least one clam in each group of six tested contained ca
   22 mug g-1 reflecting the clam-to-clam variability in their natural
   habitat. It appears that razor clams are able to depurate domoic acid in
   their natural environment, but may maintain a low level of domoic acid
   for long periods.},
